.layout-content-body
  .title-bar
    .title-bar-actions
      %select.selectpicker.dropdown{"data-dropdown-align-right" => "true", "data-style" => "btn-default btn-sm", "data-width" => "fit", :name => "period"}
        %option{:value => "today"} Today
        %option{:value => "yesterday"} Yesterday
        %option{:selected => "selected", :value => "last_7d"} Last 7 days
        %option{:value => "last_1m"} Last 30 days
        %option{:value => "last_3m"} Last 90 days
    %h1.title-bar-title
      %span.d-ib Dashboard
      %span.d-ib
        %a.title-bar-shortcut{"data-container" => "body", "data-placement" => "right", "data-toggle" => "tooltip", "data-toggle-text" => "Remove from shortcut list", "data-trigger" => "hover", :href => "#", :title => "Add to shortcut list"}
          %span.sr-only Add to shortcut list
  .row.gutter-xs
    .col-md-12
      .row.gutter-xs
        .col-md-12
          .card
            .card-header
              .card-actions
                %button.card-action.card-toggler{:title => "Collapse", :type => "button"}
                %button.card-action.card-reload{:title => "Reload", :type => "button"}
                %button.card-action.card-remove{:title => "Remove", :type => "button"}
              %strong Report
            .card-body{"data-toggle" => "match-height"}
              %table.table.table-borderless.table-middle
                %tbody
                  %tr
                    %td.col-xs-1 1.
